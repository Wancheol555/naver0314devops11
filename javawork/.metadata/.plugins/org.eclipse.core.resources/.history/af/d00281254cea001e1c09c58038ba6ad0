		//setter
		public void setSang(String sang) 
		{
			this.sang=sang; 
		}
		
		public void setSu(int su)
		{
			this.su=su;
		}
		public void setDan(int dan)
		{
			this.dan=dan;
		}
		public void setSangpum(String sang ,int su,int dan)
		{
			//매서드안에서 같은 멤버 매서드는 this로 호출이 가능
			this.setSang(sang);
			this.setSu(su);
			this.setDan(dan);
			
		}
		//getter 
		public String getSang()
		{
			return this.sang;
		}
		public int getSu()
		{
			return this.su;
		}
		public int getDan()
		{
			return this.dan;
		}
}

