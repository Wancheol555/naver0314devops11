package day0325;

public class Employee {
	
		private String name;
		private String position;
		private int age;

		
		public Employee(String name, String position, int age) {
			super();
			this.name = name;
			this.position = position;
			this.age = age;
		}

		public String getName() {
			return name;
		}

		public void setName(String name) {
			this.name = name;
		}

		public String getPosition() {
			return position;
		}

		public void setPosition(String position) {
			this.position = position;
		}

		public int getAge() {
			return age;
		}

		public void setAge(int age) {
			this.age = age;
		}
		
	
		
		//추가로 필요한 매서드 구현하기
		
//		public void setScore(int java,int spring)
//		{
//			this.java=java;
//			this.spring=spring;
//			
//		}
//		public int getTotal()
//		{
//			return java+spring;
//		}
		

		public void setPay(int gibon,int sudang)
		{
			this.gibon=gibon;
			this.sudang=sudang;
			
		}
		
		public int getPay() {
			return gibon, sudang;
		}
		
		public int getNetPay()
		{
			return gibon+sudang;
		}
		
		public int getSudang() {
			return sudang;
		}

		public void setSudang(int sudang) {
			this.sudang = sudang;
		}
		public int getGibon() {
			return gibon;
		}

		public void setGibon(int gibon) {
			this.gibon = gibon;
		}
		
}
